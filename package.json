{
  "name": "bicycles",
  "version": "1.0.0",
  "description": "Лендинг магазина велосипедов",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "^1.0.0",
    "autoprefixer": "^10.2.5",
    "browser-sync": "^2.26.14",
    "gulp": "^4.0.2",
    "gulp-imagemin": "7.1.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "9.0.0",
    "postcss": "^8.3.0",
    "postcss-csso": "5.0.1",
    "gulp-posthtml": "^3.0.4",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "4.1.0",
    "gulp-sourcemaps": "3.0.0",
    "gulp-svgstore": "^7.0.1",
    "gulp-webp": "^4.0.1",
    "pixel-glass": "^1.0.13",
    "posthtml-include": "^1.2.0",
    "stylelint": "13.12.0",
    "stylelint-config-htmlacademy": "0.1.x",
    "eslint": "7.32.0",
    "eslint-config-htmlacademy": "4.0.2"
